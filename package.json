{
  "name": "qordoba-file-helper",
  "version": "0.0.0",
  "description": "helps manage file uploads, update and downloads to & from qordoba",
  "main": "index.js",
  "scripts": {
    "uploadNew": "gulp build && node lib/bin/uploadNew.js",
    "uploadModified": "gulp build && node lib/bin/uploadModified.js",
    "download": "gulp build && node lib/bin/download.js",
    "update": "gulp build && node lib/bin/update.js",
    "test": "gulp build && mocha --timeout 10000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "mocha": "^3.2.0",
    "npm": "^4.1.2"
  },
  "dependencies": {
    "qordoba-js-sdk": "0.0.2"
  },
  "bin": {
    "download": "node lib/bin/download.js",
    "update": "node lib/bin/update.js",
    "uploadModified": "node lib/bin/uploadModified.js",
    "uploadNew": "node lib/bin/uploadNew.js"
  }
