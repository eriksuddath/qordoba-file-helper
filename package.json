{
  "name": "qordoba-file-helper",
  "version": "0.1.6",
  "description": "helps manage file uploads, update and downloads to & from qordoba",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "gulp build",
    "uploadNew": "gulp build && node bin/uploadNew.js",
    "uploadModified": "gulp build && node bin/uploadModified.js",
    "download": "gulp build && node bin/download.js",
    "update": "gulp build && node bin/update.js",
    "test": "gulp build && mocha --timeout 10000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "mocha": "^3.2.0",
    "npm": "^4.1.2"
  },
  "dependencies": {
    "qordoba-js-sdk": "0.0.2",
    "qordoba-nodejs-sdk": "0.0.3"
  },
  "bin": {
    "qordoba-download": "bin/download.js",
    "qordoba-update": "bin/update.js",
    "qordoba-uploadModified": "bin/uploadModified.js",
    "qordoba-uploadNew": "bin/uploadNew.js",
    "qordoba-uploadAll": "bin/uploadAll.js"
  }
}
